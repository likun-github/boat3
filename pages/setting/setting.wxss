  z-index: 11;
  min-width: 200rpx;
  min-height: 200rpx;
  letter-spacing: 0;
  word-wrap: break-word;
	animation-name:bounceInDown;
	animation-duration:1s;
	animation-timing-function:linear;
	animation-delay:0;
	animation-iteration-count:1;
	animation-direction:normal;
}
