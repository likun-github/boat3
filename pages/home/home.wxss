.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 600rpx;
  overflow: hidden;
  position: fixed;
  top: 60%;
  left: 0;
  z-index: 9999;
  background: white;
  display: flex;
  border-radius: 36rpx;
  flex-direction: column;
  font-size: 35rpx;
  height: 395rpx;
  margin-left: 75rpx;
}

.modal-title {
  padding-top: 50rpx;
  font-size: 36rpx;
